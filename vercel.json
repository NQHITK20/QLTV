{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/web-ui/(.*)",
        "dest": "/web-ui/$1"
      },
      {
        "src": "/admin-ui/(.*)",
        "dest": "/admin-ui/$1"
      }
    ]
  }
  